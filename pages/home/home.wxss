/* 商品价格和市场价 */

.salemarket, .salemarket .oldprice, .salemarket .sell {
    float: left;
}

.salemarket {
    white-space: nowrap;
    font-family: "microsoft yahei";
    color: #fb1438;
    font-size: 32rpx;
}

.salemarket .sell text, .salemarket .oldprice text {
    font-weight: 600;
    font-family: arial;
}

.salemarket .oldprice {
    font-size: 28rpx;
    color: #bdbdbd;
    text-decoration: line-through;
    margin-left: 8rpx;
    margin-top: 12rpx;
}

.salemarket .oldprice text {
    font-weight: normal;
}

.salemarket + .product-shop {
    float: right;
    position: absolute;
    right: 0;
    background: #fff;
}

.shop-head {
    height: 192rpx;
    overflow: hidden;
    position: relative;
    max-width: 100%;
    margin: 0 auto;
}

.shop-head-bg {
    position: absolute;
    z-index: 1;
    top: 0;
    left: -20%;
    width: 140%;
    height: 192rpx;
    -webkit-filter: blur(30rpx);
    filter: blur(30rpx);
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}

.shop-head-inner {
    height: 192rpx;
    padding: 48rpx 24rpx;
    position: relative;
    z-index: 2;
    background: rgba(0, 0, 0, 0.3);
    box-sizing:border-box;
}

.shop-logo {
    width: 96rpx;
    height: 96rpx;
    border-radius: 10rpx;
    border: 2rpx solid #fff;
    overflow: hidden;
    display: inline-block;
    margin-right: 16rpx;
    vertical-align: top;
}

.shop-name {
    font-size: 32rpx;
    color: #fff;
    line-height: 96rpx;
    display: inline-block;
}
