.merge-do-list .item:first-child:before, .merge-over-sign, .merge-fail-sign,
.merge-success-sign {
    background: url(http://demo.himall.kuaidiantong.cn/Areas/Mobile/Templates/Default/images/merge.png) no-repeat;
    background-size: 640rpx 440rpx;
}

.merge-rule {
    padding: 28rpx 24rpx;
    font-size: 26rpx;
    line-height: 36rpx;
    color: #6b6c6e;
    background-color: #f9f9f9;
}

.merge-go-tip {
    padding: 30rpx 0 24rpx;
    font-size: 26rpx;
    font-weight: bold;
    color: #494e52;
    letter-spacing: 1rpx;
}

.merge-go-tip view {
    display: inline-block;
}

.merge-go-tip .merge-highlight {
    color: #3cbcba;
    margin: 0 8rpx;
}

.merge-over-sign, .merge-fail-sign, .merge-success-sign {
    position: absolute;
    z-index: 1;
    width: 192rpx;
    height: 192rpx;
    top: 32rpx;
    right: 32rpx;
}

.merge-over-sign {
    background-position: -320rpx 0;
}

.merge-fail-sign {
    background-position: -8rpx 0;
}

.merge-success-sign {
    background-position: -450rpx 0;
}

.merge-do-list {
    margin-top: 16rpx;
}

.merge-do-list .item {
    text-align: center;
    width: 20%;
    padding: 16rpx;
    position: relative;
    box-sizing: border-box;
    float: left;
}

.merge-do-list .avatar {
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border: 3rpx solid transparent;
    border-radius: 100%;
    display: inline-block;
}

.merge-do-list .avatar image {
    width: 100%;
    height: 100%;
}

.merge-do-list .item:first-child .avatar {
    border-color: #ffc015;
}

.merge-do-list .item:first-child:before {
    position: absolute;
    top: -5rpx;
    left: 50%;
    margin-left: -20rpx;
    width: 48rpx;
    height: 26rpx;
    content: '';
    background-position: -150rpx -348rpx;
}

.merge-do-list .item .name {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.merge-go, .merge-do {
    padding: 0 24rpx;
    background-color: #fff;
    position: relative;
}

.merge-go .item {
    padding-top: 24rpx;
}

.merge-go .item+.item {
    border-top: 1rpx solid #f0f0f0;
}

.merge-go .info view {
    display: inline-block;
}

.merge-go .info {
    width: 100%;
}

.merge-go .info .avatar {
    position: relative;
    width: 80rpx;
    height: 80rpx;
    vertical-align: middle;
    border-radius: 100%;
}

.merge-go .info .avatar image {
    width: 100%;
    height: 100%;
    border-radius: 100%;
}

.merge-go .info .leader {
    height: 30rpx;
    padding: 0 16rpx;
    margin: 0 26rpx 0 16rpx;
    font-size: 20rpx;
    line-height: 30rpx;
    color: #fff;
    background-color: #fcc41b;
    border-radius: 16rpx;
}

.merge-go .info .name {
    padding-right: 24rpx;
    font-size: 24rpx;
    color: #494e52;
}

.merge-go .goto {
    padding: 24rpx 0;
    display: flex;
}

.merge-go .goto view {
    flex: 1;
}

.text-center {
    text-align: center;
}

.text-right {
    text-align: right;
}

.merge-go .goto .text-right {
    color: #fb1438;
}

.merge-go .goto .text-left text {
