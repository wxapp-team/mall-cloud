.hdonglist {
    padding: 32rpx 24rpx;
    line-height: 40rpx;
    font-size: 32rpx;
    position: relative;
    background: #fff;
    border-bottom: 1rpx solid #f0f0f0;
    height: 40rpx;
}

.hdonglist:last-child {
    border: none;
}

.hdonglist::after, .hdonglist::before {
    border: 6px solid transparent;
    border-left: 6px solid #fff;
    width: 0;
    position: absolute;
    right: 10px;
    height: 0;
    top: 38rpx;
    content: ' ';
}

.hdonglist::before {
    border-left-color: #bbb;
    right: 8px;
}

.hdonglist .red {
    color: red;
}

.dashline {
    border-top: 1rpx dashed #6b6c6e;
}

