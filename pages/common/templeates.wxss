.temp4 .product {
    position: relative;
    float: left;
    width: 50%;
    padding: 0 0 8rpx;
    overflow: hidden;
    vertical-align: top;
    box-sizing: border-box;
}

.temp4 .product .wrapper {
    overflow: hidden;
    background-color: #fff;
}

.temp4 .product image {
    width: 100%;
    height: 375rpx;
}

.temp4 .product .price {
    width: 100%;
    height: auto;
    line-height: normal;
    padding: 20rpx 16rpx 20rpx;
    color: #fc2236;
    font-size: 28rpx;
    box-sizing: border-box;
}

.temp4 .product .price view {
    float: right;
    height: 44rpx;
    width: 44rpx;
    font-size: 26rpx;
    line-height: 44rpx;
    color: #fff;
    text-align: center;
    background-color: #fc1329;
    border-radius: 50%;
}

.temp4 .product .price text {
    display: block;
}

.temp4 .product .price text.title {
    height: auto;
    color: #494e52;
    font-size: 32rpx;
    line-height: 52rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    margin-bottom: 16rpx;
}

.temp4.layout1 .product:nth-of-type(odd),.temp4.layout3 .product:nth-child(3n+2),.temp4.layout5 .product:nth-of-type(odd) {
    padding-left: 0;
    padding-right: 4rpx;
}

.temp4.layout1 .product:nth-of-type(even),.temp4.layout3 .product:nth-child(3n),.temp4.layout5 .product:nth-of-type(even) {
    padding-right: 0;
    padding-left: 4rpx;
}

.temp4.layout2 .product,.temp4.layout3 .product:nth-child(3n+1),.temp4.layout4 .product {
    width: 100%;
}

.temp4.layout2 .product image,.temp4.layout3 .product:nth-child(3n+1) image {
    height: 750rpx;
}

.temp4.layout2 .product .price,.temp4.layout3 .product:nth-child(3n+1) .price {
    font-size: 32rpx;
}

.temp4.layout2 .product .price view,.temp4.layout3 .product:nth-child(3n+1) .price view{
    margin-top: 4rpx;
    margin-left: 16rpx;
}

.temp4.layout3 .product:nth-child(3n+2) .price view,.temp4.layout3 .product:nth-child(3n) .price view,.temp4.layout4 .product .price view,.temp4.layout5 .product .price view{
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
}

.temp4 .product .price text.title.ws-normal{
    white-space: normal;
}

.temp4 .product .price text.title.two-text-overflow{
    white-space: normal;
    display: -webkit-box;
}

.temp4 .product .price text.title.two-text-overflow{
    height: 104rpx;
} 

