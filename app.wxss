}

.comm-stand {
    margin: 55rpx;
    background: #fff;
    border-radius: 12rpx;
    box-shadow: 0rpx 0rpx 15rpx #ccc;
    position: fixed;
    top: 15%;
    left: 0rpx;
    right: 0rpx;
}

.comm-stand-con {
    padding: 32rpx 32rpx 20rpx 32rpx;
    position: relative;
    overflow: hidden;
}

.comm-stand .comm-item {
    overflow: hidden;
    margin-bottom: 12rpx;
    line-height: 40rpx;
    clear: both;
}

.comm-attr {
    overflow: hidden;
}

.comm-stand .comm-name {
    font-size: 32rpx;
    line-height: 40rpx;
    padding-bottom: 24rpx;
    margin-bottom: 24rpx;
    border-bottom: 1rpx solid #e0e0e0;
}

.comm-stand .comm-name .comm-txt {
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 453rpx;
}

.comm-stand .comm-bd {
    max-height: 450rpx;
    overflow-y: auto;
}

.comm-stand .comm-icon {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    top: 24rpx;
    right: 28rpx;
}

.comm-stand .comm-icon image {
    width: 40rpx;
    height: 40rpx;
}

.comm-item .comm-title {
    font-size: 28rpx;
    line-height: 44rpx;
    margin-bottom: 12rpx;
}

.comm-item .comm-attr text {
    border: 1rpx solid #fff;
    padding: 0 24rpx;
    line-height: 56rpx;
    display: block;
    float: left;
    margin-top: 12rpx;
    margin-bottom: 12rpx;
    margin-right: 24rpx;
    white-space: nowrap;
    border-radius: 5px;
    background: #f5f5f5;
    font-size: 24rpx;
    max-width: 500rpx;
    overflow: hidden;
}

.comm-item .comm-attr text.active {
    border: 1rpx solid #fb1438;
    background: #fb1438;
    color: #fff;
}

.comm-item .comm-attr text.disabled {
    opacity: 0.5;
    color: #999;
    background: #f5f5f5;
    border-color: #f5f5f5;
