/*加入购物车标志*/

.shop_box {
    position: fixed;
    bottom: 310rpx;
    right: 24rpx;
    width: 96rpx;
    height: 96rpx;
    z-index: 99;
    background: #979797;
    border-radius: 50%;
    border: 1rpx solid #7c7c7c;
    line-height: 96rpx;
    text-align: center;
}

.shop_box .shop_icon {
    position: relative;
    color: #fff;
}

.shop_num {
    position: absolute;
    border: 1px #ff5757 solid;
    top: 12rpx;
    right: 10rpx;
    background: #f44336;
    color: #fff;
    width: 29rpx;
    height: 29rpx;
    line-height: 29rpx;
    border-radius: 100%;
    font-size: 20rpx;
    text-align: center;
    animation-name: shop_num;
    animation: shop_num 2s alternate infinite;
    -webkit-animation: shop_num 2s alternate infinite;
    -moz-animation: shop_num 2s alternate infinite;
}

@keyframes shop_num {
    0% {
        opacity: 1;
    }

    50% {
        opacity: 1;
    }

    100% {
        opacity: 1;
    }
}

.sale-out {
    position: absolute;
    font-style: normal;
    top: 40px;
    left: 50%;
    margin-left: -40px;
    width: 80px;
    height: 80px;
    line-height: 80px;
    text-align: center;
    display: block;
    background: rgba(0, 0, 0, 0.6);
    color: #fff;
    font-size: 16px;
    border-radius: 100%;
    z-index: 5;
}

/*弹框*/

.dialog {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: scale(0, 0) translate(-50%, -50%);
    transform-origin: 0 0 0;
    z-index: 99;
    background: #fff;
    border-radius: 16rpx;
    padding: 32rpx;
    width: 654rpx;
    overflow: hidden;
    opacity: 0;
    transition: all 0.3s;
}

.dialog.active {
    transform: scale(1, 1) translate(-50%, -50%);
    opacity: 1;
}

.dialog-title {
    font-size: 32rpx;
    line-height: 32rpx;
    margin-top: 8rpx;
}

.dialog-title .text {
    max-width: 456rpx;
    display: inline-block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.dialog-title .iconfont {
    color: #bdbdbd;
    float: right;
}

.dialog-content {
    padding-top: 32rpx;
}

.cover {
    background: rgba(0, 0, 0, 0.2);
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
    bottom: 0;
    z-index: 98;
}

.full-scroll {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
}

.goTop {
    position: fixed;
    bottom: 100rpx;
    right: 24rpx;
    background: rgba(0, 0, 0, 0.48);
    border-radius: 50%;
    overflow: hidden;
